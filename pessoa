public class pessoa{
  public static void main(String[] args){
    // Code inside the main method
  }
  // Instance variables
  public String nome;
  public int idade;
  public int cpf;
  
  public pessoa(String nome, int idade, int cpf){
  this.nome = nome;
  this.idade = idade;
  this.cpf = cpf;
  }
  // Setter methods
  public void setNome(String nome){
    this.nome=nome;
  }
  public void setIdade(int idade){
    this.idade=idade;
  }
  public void setCpf(int cpf){
    this.cpf=cpf;
  }
  // Getter methods
  public String getNome(){
    return nome;
  }
  public int getIdade(){
    return idade;
  }
  public int getCpf(){
    return cpf;
  }
}
